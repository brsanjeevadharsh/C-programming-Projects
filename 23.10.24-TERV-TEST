for(int i=0,j=0;i<n,j<n;i++,j++)
{
    if(a[i]>b[j])
    {
        num++;
    }
    else if(a[i]<b[j])
    {
        num2++;
    }
    else
    {
        num+=0;
        num2+=0;
    }
}
